{
  "extends": "@tsconfig/react-native/tsconfig.json",

  
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "lib": ["esnext"],
      "strict": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "jsx": "react-native"
    },
    "exclude": ["node_modules"]
  
}
